[access]
	inheritFrom = All-Projects
[submit]
	action = rebase always

[label "Code-Review"]
	value = -1 Don't submit as-is
	value = 0 No score
	value = +1 Looks good to me
	defaultValue = 0
	function = MaxWithBlock
	copyMinScore = true
	copyMaxScore = true
	copyAllScoresOnTrivialRebase = true
	copyAllScoresIfNoCodeChange = true
	ignoreSelfApproval = true
[label "Commit-Queue"]
	function = MaxWithBlock
	defaultValue = 0
[label "Verified"]
	function = MaxWithBlock
	defaultValue = 0

[access "refs/for/refs/*"]
	push = group Logged-in Public Users
	create = group Logged-in Public Users
	forgeAuthor = group use-cria-project-chromium-committers-instead
	exclusiveGroupPermissions = addPatchSet
	addPatchSet = group use-cria-project-chromium-committers-instead
[access "refs/heads/*"]
	label-Code-Review = -1..+1 group use-cria-project-chromium-committers-instead
	submit = group use-cria-project-chromium-committers-instead
	push = group use-cria-project-chromium-committers-instead
[project]
	description = Low level C API for making direct Linux syscalls.
[access "refs/for/refs/heads/master"]
	exclusiveGroupPermissions = push
	push = deny group Anonymous Public Users
	push = deny group Logged-in Public Users
[access "refs/heads/master"]
	exclusiveGroupPermissions = push
	push = deny group Anonymous Public Users
	push = deny group Logged-in Public Users
[receive]
	requireContributorAgreement = true
